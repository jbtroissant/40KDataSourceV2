            {
              "name": "Oath of Rynn",
              "typeId": "9cc3-6d83-4dd3-9b64",
              "typeName": "Abilities",
              "hidden": "false",
              "id": "5324-763c-61f5-9e4b",
              "characteristics": {
                "characteristic": {
                  "name": "Description",
                  "typeId": "9b8f-694b-e5e-b573",
                  "_text": "Once per battle, at the start of either player’s Command phase, this model can use this ability. When it does, until the end of the turn, add 1 to the Attacks characteristic of weapons equipped by models in this model’s unit."
                }
              }
            },
            {
              "name": "To the Last",
              "typeId": "9cc3-6d83-4dd3-9b64",
              "typeName": "Abilities",
              "hidden": "false",
              "id": "b20-ca96-c186-27dc",
              "characteristics": {
                "characteristic": {
                  "name": "Description",
                  "typeId": "9b8f-694b-e5e-b573",
                  "_text": "While this model is leading a unit, each time a model in that unit makes an attack, add 1 to the Hit roll if that unit is below its Starting Strength, and add 1 to the Wound roll as well if that unit is Below Half-strength."
                }
              }
            },
            {
              "name": "Leader",
              "typeId": "9cc3-6d83-4dd3-9b64",
              "typeName": "Abilities",
              "hidden": "false",
              "id": "39c9-ff24-66c5-c3aa",
              "characteristics": {
                "characteristic": {
                  "name": "Description",
                  "typeId": "9b8f-694b-e5e-b573",
                  "_text": "This model can be attached to the following units:\n\n■ Company Heroes\n■ Bladeguard Veteran Squad\n■ Sternguard Veteran Squad\n■ Tactical Squad"
                }
              }
            },
            {
              "name": "Inspiring Commander",
              "typeId": "9cc3-6d83-4dd3-9b64",
              "typeName": "Abilities",
              "hidden": "false",
              "id": "1544-468d-6b7c-a26b",
              "characteristics": {
                "characteristic": {
                  "name": "Description",
                  "typeId": "9b8f-694b-e5e-b573",
                  "_text": "If you include this model in your army, until the end of the battle, non-Character models in Sternguard\nVeteran Squad units from your army have an Objective Control characteristic of 2 while they are not Battle-shocked."
                }
              }
            }
          ]
